{
  "manifest_version": 2,
  "name": "Tab spy",
  "version": "1.1",
  "description": "A spy sending the tab events to a server for data collection and mining.",
  "icons": {"128": "images/icon-128.png"},
  "browser_action": {
    "default_icon": "images/icon-19.png",
    "default_title": "Tab spy"
  },
  "background": {"scripts": ["background.js"]},
  "permissions": ["tabs", "http://*/*" ]
}
